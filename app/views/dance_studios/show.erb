<div class="container is-fluid">
  <div class="content">
    <h1 class='title is-1'><%= current_user.studio_name %></h1>
    <h2 class="subtitle is-3"><%= current_user.owner_name %></h2>
  </div>
  <div class='buttons'>
    <div class='button'>
      <%= link_to 'My Dancers', dance_studio_dancers_path(current_user) %>
    </div>
    <div class='button'>
      <%= link_to 'My Costume Assignments', dance_studio_costume_assignments_path(current_user) %>
    </div>
    <div class='button'>
      <%= link_to 'Edit Account Info', edit_dance_studio_path(current_user) %>
    </div>
    <div class='button'>
      <%= link_to 'Delete Account', dance_studio_path(current_user), method: :delete %>
    </div>
  </div>
</div>
