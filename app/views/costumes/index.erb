<div class='container is-fluid'>
  <div class='content'>
    <h1 class='title'><%= current_user.studio_name %> Costumes</h1>
  </div>
  <div class="columns">
    <% @costumes.each do |costume| %>
      <div class="column">
        <figure class="image is-96x96">
          <%= link_to image_tag(costume.picture, width: 100), costume_path(costume) %>
        </figure>
      </div>
    <% end %>
  </div>
</div>