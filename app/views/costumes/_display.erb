<div class='container is-fluid has-text-centered'>
  <div class='columns is-desktop'>
    <% @count = 0 %>
    <% @costumes.each do |costume| %>
      <% costume_path = set_costume_path(costume, @season) %>
      <div class='column is-2 is-narrow-desktop'>
        <%= render 'costumes/create_rows' %>
        <div class='box'>
          <%= show_costume_with_link(costume, costume_path) %>
        </div>
      </div>
      <% @count += 1 %>
    <% end %>
  </div>
</div>